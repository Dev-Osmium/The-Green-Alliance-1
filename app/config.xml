<?xml version='1.0' encoding='utf-8'?>
<widget id="org.team4909.thegreenalliance" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TGA</name>
    <description>
        Team 4909's 2018+ Scouting System
    </description>
    <author email="roshanravi10@gmail.com">
        Roshan Ravi
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <engine name="android" spec="^6.4.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="tga-bluetooth-2018" spec="file:bluetooth-plugin" />
</widget>
